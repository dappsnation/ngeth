{
  "projectType": "application",
  "root": "e2e/hardhat-gui",
  "sourceRoot": "e2e/hardhat-gui/src",
  "prefix": "eth",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/e2e/hardhat-gui/browser",
        "index": "e2e/hardhat-gui/src/index.html",
        "main": "e2e/hardhat-gui/src/main.ts",
        "polyfills": "e2e/hardhat-gui/src/polyfills.ts",
        "tsConfig": "e2e/hardhat-gui/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "e2e/hardhat-gui/src/favicon.ico",
          "e2e/hardhat-gui/src/assets"
        ],
        "styles": [
          "e2e/hardhat-gui/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "e2e/hardhat-gui/src/environments/environment.ts",
              "with": "e2e/hardhat-gui/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "hardhat-gui:build:production"
        },
        "development": {
          "browserTarget": "hardhat-gui:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "hardhat-gui:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "e2e/hardhat-gui/src/**/*.ts",
          "e2e/hardhat-gui/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/e2e/hardhat-gui"
      ],
      "options": {
        "jestConfig": "e2e/hardhat-gui/jest.config.js",
        "passWithNoTests": true
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/e2e/hardhat-gui/server",
        "main": "e2e/hardhat-gui/server.ts",
        "tsConfig": "e2e/hardhat-gui/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "e2e/hardhat-gui/src/environments/environment.ts",
              "with": "e2e/hardhat-gui/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "hardhat-gui:build:development",
          "serverTarget": "hardhat-gui:server:development"
        },
        "production": {
          "browserTarget": "hardhat-gui:build:production",
          "serverTarget": "hardhat-gui:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routes": [
          "/"
        ]
      },
      "configurations": {
        "production": {
          "browserTarget": "hardhat-gui:build:production",
          "serverTarget": "hardhat-gui:server:production"
        },
        "development": {
          "browserTarget": "hardhat-gui:build:development",
          "serverTarget": "hardhat-gui:server:development"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "tags": []
}
