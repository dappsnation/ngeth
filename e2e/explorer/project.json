{
  "projectType": "application",
  "root": "e2e/explorer",
  "sourceRoot": "e2e/explorer/src",
  "prefix": "eth",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/e2e/explorer/browser",
        "index": "e2e/explorer/src/index.html",
        "main": "e2e/explorer/src/main.ts",
        "polyfills": "e2e/explorer/src/polyfills.ts",
        "tsConfig": "e2e/explorer/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["e2e/explorer/src/favicon.ico", "e2e/explorer/src/assets"],
        "styles": ["e2e/explorer/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "e2e/explorer/src/environments/environment.ts",
              "with": "e2e/explorer/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "explorer:build:production"
        },
        "development": {
          "browserTarget": "explorer:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "explorer:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "e2e/explorer/src/**/*.ts",
          "e2e/explorer/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/e2e/explorer"],
      "options": {
        "jestConfig": "e2e/explorer/jest.config.js",
        "passWithNoTests": true
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/e2e/explorer/server",
        "main": "e2e/explorer/server/main.ts",
        "tsConfig": "e2e/explorer/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "e2e/explorer/src/environments/environment.ts",
              "with": "e2e/explorer/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "explorer:build:development",
          "serverTarget": "explorer:server:development"
        },
        "production": {
          "browserTarget": "explorer:build:production",
          "serverTarget": "explorer:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "production": {
          "browserTarget": "explorer:build:production",
          "serverTarget": "explorer:server:production"
        },
        "development": {
          "browserTarget": "explorer:build:development",
          "serverTarget": "explorer:server:development"
        }
      },
      "defaultConfiguration": "production"
    },
    "hardhat-build": {
      "options": {
        "config": "e2e/explorer/hardhat.config.ts",
        "tsconfig": "e2e/explorer/tsconfig.hardhat.json"
      },
      "executor": "@ngeth/hardhat:build"
    },
    "hardhat-serve": {
      "options": {
        "config": "e2e/explorer/hardhat.config.ts",
        "tsconfig": "e2e/explorer/tsconfig.hardhat.json"
      },
      "executor": "@ngeth/hardhat:serve"
    },
    "hardhat-test": {
      "options": {
        "config": "e2e/explorer/hardhat.config.ts",
        "tsconfig": "e2e/explorer/tsconfig.hardhat.json"
      },
      "executor": "@ngeth/hardhat:test"
    }
  },
  "tags": []
}
