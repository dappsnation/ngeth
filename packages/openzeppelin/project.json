{
  "projectType": "library",
  "root": "packages/openzeppelin",
  "sourceRoot": "packages/openzeppelin/src",
  "prefix": "ngeth",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "dist/packages/openzeppelin"
      ],
      "options": {
        "project": "packages/openzeppelin/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/openzeppelin/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/openzeppelin/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/openzeppelin"
      ],
      "options": {
        "jestConfig": "packages/openzeppelin/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/openzeppelin/src/**/*.ts",
          "packages/openzeppelin/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
